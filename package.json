{
  "name": "chronoshield",
  "version": "2.0.1",
  "private": true,
  "license": "MIT",
  "author": "Q3 Labs",
  "workspaces": {
    "packages": [
      "packages/snfoundry",
      "packages/nextjs",
      "packages/auco",
      "packages/circuit"
    ]
  },
  "scripts": {
    "chain": "yarn workspace @chronoshield/snfoundry chain",
    "clean": "yarn workspace @chronoshield/snfoundry clean",
    "compile": "yarn workspace @chronoshield/snfoundry compile",
    "deploy": "yarn workspace @chronoshield/snfoundry deploy",
    "deploy:clear": "yarn workspace @chronoshield/snfoundry deploy:clear",
    "deploy:no-reset": "yarn workspace @chronoshield/snfoundry deploy --no-reset",
    "format": "yarn workspace @chronoshield/nextjs format && yarn workspace @chronoshield/snfoundry format",
    "format:check": "yarn workspace @chronoshield/nextjs format:check && yarn workspace @chronoshield/snfoundry format:check",
    "next:check-types": "yarn workspace @chronoshield/nextjs check-types",
    "next:lint": "yarn workspace @chronoshield/nextjs lint",
    "prepare": "husky",
    "start": "yarn workspace @chronoshield/nextjs dev",
    "test": "yarn workspace @chronoshield/snfoundry test",
    "test:nextjs": "yarn workspace @chronoshield/nextjs test",
    "vercel": "yarn workspace @chronoshield/nextjs vercel",
    "vercel:yolo": "yarn workspace @chronoshield/nextjs vercel:yolo",
    "verify": "yarn workspace @chronoshield/snfoundry verify",
    "auco:dev": "yarn workspace @chronoshield/auco dev",
    "auco:start": "yarn workspace @chronoshield/auco start",
    "circuit:build": "yarn workspace @chronoshield/circuit build",
    "circuit:exec": "yarn workspace @chronoshield/circuit exec",
    "circuit:gen-verifier": "yarn workspace @chronoshield/circuit gen-verifier",
    "circuit:gen-vk": "yarn workspace @chronoshield/circuit gen-vk",
    "circuit:prove": "yarn workspace @chronoshield/circuit prove"
  },
  "dependencies": {
    "postcss": "^8.4.38"
  },
  "devDependencies": {
    "daisyui": "^4.7.3",
    "husky": "^9.0.11",
    "pinst": "^3.0.0",
    "prettier": "^3.2.5"
  },
  "packageManager": "yarn@3.2.3",
  "overrides": {
    "chalk": "5.3.0",
    "strip-ansi": "7.1.0",
    "color-convert": "2.0.1",
    "color-name": "1.1.4",
    "is-core-module": "2.13.1",
    "error-ex": "1.3.2",
    "has-ansi": "5.0.1"
  }
}
